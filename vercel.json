{
    "version":2,
    "builds":[
        {
            "src":"app.js",
            "use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "/app.js"
        }
    ],
    "crons": [{
        "path": "/routes/cron",
        "schedule": "0 0 1 12 *"
    }]
}